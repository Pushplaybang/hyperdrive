{
  "name": "comfusion/hyperdrive",
  "description": "The fastest way to load pages in WordPress.",
  "keywords": ["wordpress", "wp", "performance", "plugin", "hyperdrive"],
  "homepage": "http://hyperdrive.habd.as",
  "license": "GPL-3.0",
  "authors": [{
    "name": "Josh Habdas",
    "email": "josh@habd.as",
    "homepage": "https://habd.as"
  }],
  "scripts": {
    "lint": "phpcs --standard=phpcs.ruleset.xml $(find . -name '*.php')"
  },
  "type": "wordpress-plugin",
  "require": {
    "php": ">=5.6",
    "composer/installers": "^1.3"
  },
  "autoload": {
    "files": [
      "src/hyperdrive.php"
    ]
  },
  "require-dev": {
    "codeclimate/php-test-reporter": "dev-master",
    "kahlan/kahlan": "^3.1"
  },
  "extra": {
    "branch-alias": {
      "dev-master": "1.0.x-dev"
    }
  }
}
